@page "/storybook"
@model LearnEnglish.Pages.storybook.storybookModel

@{ Layout = null; }

<!DOCTYPE html>
<html lang="en" style="overflow:hidden;">
<head>
    <title>Learn English Third Book</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" href="favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" href="../favicon.ico" type="image/x-icon" />
    <meta charset="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
    <meta http-equiv="Cache-Control" content="no-cache, no-store, must-revalidate" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Expires" content="0" />

    <link rel="stylesheet" type="text/css" href="~/css/storybook/jplayer.blue.monday.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/storybook/storybook.min.css">
    <link rel="stylesheet" type="text/css" href="~/css/storybook/apps.min.css">
    <link rel="icon" type="image/x-icon" href="https://majemastoragetest.blob.core.windows.net/learn-english-test/ebook/favicon.ico" />

</head>

<body id="main-body">

    <div id="main">
        <div id="userTop"></div>
        <div id="toolBar"></div>
        <div id="userLeft"></div>
        <div class="depth left" style="width: 0px; background-image: url(https://majemastoragelive.blob.core.windows.net/learn-english-test/content/storybooks/img/pages-depth-left.png);position: absolute;height: 100%;z-index: 1;height: 865px;left: 188px;top: 8px;background-repeat: no-repeat;background-size:cover;"></div>
        <div class="depth right" style="width: 0px; background-image: url(https://majemastoragelive.blob.core.windows.net/learn-english-test/content/storybooks/img/pages-depth-right.png);position: absolute;height: 100%;z-index: 1;height: 865px;right: 218px;top: 11px;background-repeat: no-repeat;background-size:cover;"></div>
        <div id="container"></div>
        <div id="userRight"></div>
        <div id="userBottom"></div>
    </div>

    <div class="footer" id="footer">
        <div class="footerContent" id="footerContent" style="width:100%;height:100%;visibility: hidden;">

            <div id="jplayer">
                <div id="player" class="">

                    <div id="jquery_jplayer"></div>
                    <div class="jp-single-player noselect">
                        <div class="jp-interface">
                            <div class="jp-controls ">
                                <div><a href="#" id="jplayer_pause" class="jp-pause sprite sprite-menu_pause" tabindex="1" style="left: 65px; display: none;">pause</a></div>
                                <div><a href="#" id="jplayer_play" class="jp-play sprite sprite-menu_start" style="left: 65px; display: block;" tabindex="1">play</a></div>
                                <div><a href="#" id="jplayer_stop" class="jp-stop sprite sprite-menu_stop" tabindex="1">stop</a></div>
                                <div><a href="#" class="jp-caption activate sprite sprite-T1" tabindex="1">toggleCaption</a></div>
                                <div><a href="#" class="jp-caption deactivate sprite sprite-T2" tabindex="1">toggleCaption</a></div>
                                <div>
                                    <div id="jplayer_volume_bar" class="jp-volume-bar sprite sprite-slider_bgOff">
                                        <div id="jplayer_volume_bar_value" class="jp-volume-bar-value sprite sprite-slider_bgFull" style="width: 50%;"></div>
                                    </div><a href="#" id="jplayer_volume_min" class="jp-volume-min sprite sprite-hogtalare_left" tabindex="1">min volume</a>
                                    <a href="#" id="jplayer_volume_max" class="jp-volume-max sprite sprite-hogtalare_right" tabindex="1">max volume</a>
                                </div>
                                <div><a href="#" id="jplayer_previous" class="jp-previous sprite sprite-menu_previous" tabindex="1">prev</a></div>
                                <div><a href="#" id="jplayer_next" class="jp-next sprite sprite-menu_next" tabindex="1">next</a></div>
                                <div><a href="#" id="jplayer_home" class="jp-home sprite sprite-menu_home" tabindex="1">home</a></div>
                            </div>
                            <div class="jp-progress hidden">
                                <div id="jplayer_load_bar" class="jp-load-bar">
                                    <div id="jplayer_play_bar" class="jp-play-bar"></div>
                                </div>
                            </div>
                            <div id="jplayer_play_time" class="jp-play-time hidden"></div>
                            <div id="jplayer_total_time" class="jp-total-time hidden"></div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="button valijKapitel">
                <a href="allChaptersLink" style="" class="@ViewBag.Version">VÄLJ KAPITEL</a>
            </div>
            <div class="button valijSida">
                <a href="currentChapterLink">VÄLJ SIDA</a>
            </div>
        </div>
    </div>
    <div style="display:none;">
    </div>
    <script type="text/javascript" src="~/js/storybook/Grade3/@ViewData["story"]/pages.js"></script>

    <script src="~/js/storybook/json3.min.js" type="text/javascript"></script>
    <script src="~/js/storybook/jquery-2.1.4.min.js" type="text/javascript"></script>
    <script src="~/js/storybook/jquery-migrate-1.2.1.min.js" type="text/javascript"></script>
    <script src="~/js/storybook/jquery.jplayer.min.js" type="text/javascript"></script>
    <script src="~/js/storybook/jBook-storybook.min.js" type="text/javascript"></script>
    <script src="~/js/storybook/app.min.js" type="text/javascript"></script> 

</body>

</html>

